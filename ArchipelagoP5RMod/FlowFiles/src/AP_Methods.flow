// Procedure Index: 0
void RewardItems()
{
    int testVar;
    testVar = GET_COUNT(0x10000000 + 0);
    SET_COUNT(0x10000000 + 0, testVar + 1);
    // FLD_CAMERA_LOCK();
    SET_MSG_VAR(1, testVar, 0);
    MSG_WND_DSP();
    // GET_ITEM_WINDOW( testVar, 1, 0 );
    MSG(0);
    MSG_WND_CLS();
    // FLD_CAMERA_UNLOCK();
}

// Procedure Index: 1
void NotifyConfidantReward() 
{
    int cmmAbility = GET_COUNT(0x10000000 + 1)
    
    MSG_WND_DSP();
    if ( cmmAbility == 52 ) {
        MSG( MSG_CMM_52 ); 
    } else if ( cmmAbility == 55) {
        MSG( MSG_CMM_55 );
    }
    MSG_WND_CLS();
}